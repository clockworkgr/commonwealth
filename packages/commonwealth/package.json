    "react-dom": "^18.2.0",
    "remove-markdown": "^0.5.0",
    "safe-stable-stringify": "^2.4.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.2.0",
    "siwe": "^2.1.4",
