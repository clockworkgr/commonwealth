{
  "name": "@commonxyz/api-client",
  "version": "1.0.0",
  "type": "module",
  "description": "Common Client Proxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicommonwealth/commonwealth.git"
  },
  "bugs": {
    "url": "https://github.com/hicommonwealth/commonwealth/issues"
  },
  "homepage": "https://github.com/hicommonwealth/commonwealth#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "build": "echo Generated SDK is prebuilt",
    "clean": "rm -rf src/ && rm -f openapi.json",
    "run-test": "tsx ./test/index.ts",
    "publish-pkg": "chmod u+x scripts/publish.sh && ./scripts/publish.sh",
    "check-types": "echo No types to check",
    "generate-client": "pnpm -F commonwealth generate-oas production \"$(pwd)/openapi.json\" && fern generate",
    "generate-client-dev": "pnpm -F commonwealth generate-oas local \"$(pwd)/openapi.json\" && fern generate"
  },
  "keywords": [
    "commonwealth",
    "dao",
    "community"
  ],
  "author": "common.xyz",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2",
    "url-join": "^5.0.0"
  }
}
